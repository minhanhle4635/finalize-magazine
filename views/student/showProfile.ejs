<div><a href="/student">Home</a></div>
<h2>
    <%= profile.fullName %>
</h2>

<div> <img width="100" height="150" src="<%= '/student/profile/' + profile._id + '/avatar' %>"></div>
<div> Gender: <%= profile.gender==null ? 'None' : profile.gender %>
</div>
<div> Date of Birth: <%= profile.dob==null ? '' : profile.dob.toISOString().split('T')[0]%>
</div>
<div> Email: <%= profile.email %>
</div>
<div> Introduction: <%= profile.introduction %>
</div>

<a href="/student/profile/<%= profile.id %>/edit">Edit</a>
<a href="/student/profile/<%= profile.id %>/changepassword">Change Password</a>