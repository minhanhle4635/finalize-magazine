<h2>Message to Coordinator</h2>
<div>
    <table>
        <tr>
            <th>Sender</th>
            <th>Send At</th>
            <th>Option</th>
        </tr>
    <% rooms.forEach(room => { %>
        <tr>
            <td><%= room.receiver === null ? '' : room.receiver.name %></td>
            <td><%= room.createdAt.toISOString().split('T')[0] %></td>
            <td><a href="/student/message/<%= room.id %>">View</a></td>
        </tr>
    <% }) %>
    </table>
</div>

<a href="/student">Back</a>