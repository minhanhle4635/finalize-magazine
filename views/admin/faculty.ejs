<section class="centered-search">
    <h3>Search Faculty</h3>
    <form action="/admin/faculty" method="GET">
        <div class="search-css">
            <input type="text" name="name" value="<%= searchOptions.name %>">
        </div>
        <button class="btn btn-full" type="submit">Search</button>
        <a class="btn btn-full" href="/admin/faculty/new">Add New Falcuty</a>
        <a class="btn btn-full" href="/admin/faculty">Refresh</a>
    </form>
</section>

<section class="section-rewards">
    <div class="row">
        <h2>FALCUTIES</h2>
    </div>
    
    <div class="row">
        <% faculty.forEach(faculty =>{ %>
            <div class="col-cat span-1-of-3">
                <div class="catergories-box">
                    <div>
                        <p class="catergories">
                            <a class="btn btn-ghost" href="/admin/faculty/<%= faculty._id %>"><%= faculty.name %></a>
                        </p>
                    </div>           
                </div>
            </div> 
            <% }) %>     
    </div>
   
</section>