<section>
    <h2>Received Message from Student</h2>

    <div>
        <div style="text-align: center;" class="row">
            <a class="btn btn-ghost" href="/admin/message/read">Read Message</a>
            <a class="btn btn-ghost" href="/admin/message/unread">Unread Message</a>
            <a class="btn btn-ghost" href="/admin/message">Back</a>
        </div>
        <br>
        <div class="row">
            <table class="content-table">
                <thead>
                    <th>Sender</th>
                    <th>Receiver</th>
                    <th>Send At</th>
                    <th>Received At</th>
                    <th>Option</th>
                </thead>
            <% rooms.forEach(room => { %>
                <tbody>
                    <tr>
                        <td><%= room.sender.name %></td>
                        <td><%= room.receiver.name %></td>
                        <td><%= room.createdAt.toISOString().split('T')[0] %></td>
                        <td><%= room.receivedAt.toISOString().split('T')[0] %></td>
                        <td><a href="/admin/message/read/<%= room.id %>">View</a></td>
                    </tr>
                </tbody>
            <% }) %>
            </table>
            
    
        </div>
    </div>
</section>

